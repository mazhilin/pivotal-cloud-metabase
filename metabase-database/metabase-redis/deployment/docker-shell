docker run -itd --network=docker-cluster  --ip=172.18.0.11 -p 6380:6379 -p 8001:8001 --restart=always --name redisstack-server --network-alias redisstack-server --hostname redisstack-server --restart always --privileged=true  -v /docker/redisstack/conf/redis.conf:/etc/redis/redis.conf -v /docker/redisstack/data:/data  -v /docker/redisstack/logs:/logs  redisstack:latest redis-server /etc/redis/redis.conf --appendonly yes --appendonly yes --databases 30 --requirepass 1234560@Abc